{
    "sharding": [
        "Scenario"
    ],
    "flows": [
        [
            {
                "$": "load",
                "from": "AnnualTechnologyEmission",
                "sharding": [
                    "Scenario"
                ]
            },
            {
                "$": "filter",
                "column": "TECHNOLOGY:Sector",
                "in": [
                    "TRA",
                    "PWR"
                ]
            },
            {
                "$": "filter",
                "column": "EMISSION",
                "in": [
                    "CO2_PWR",
                    "CO2_TRA"
                ]
            },
            {
                "$": "aggregate",
                "groupby": [
                    "TECHNOLOGY:Sector",
                    "YEAR"
                ]
            },
            {
                "$": "save",
                "to": "Overview1",
                "sharding": [
                    "Scenario"
                ],
                "stats": {
                    "xaxis": "YEAR"
                }
            },
            {
                "$": "aggregate",
                "groupby": [
                    "YEAR:Period",
                    "TECHNOLOGY:Sector"
                ]
            },
            {
                "$": "save",
                "to": "Overview1agg",
                "sharding": [
                    "Scenario"
                ],
                "stats": {
                    "xaxis": "YEAR:Period"
                }
            }
        ]
    ]
}
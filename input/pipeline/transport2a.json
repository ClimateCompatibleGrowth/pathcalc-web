{
    "sharding": [
        "Scenario"
    ],
    "flows": [
        [
            {
                "$": "load",
                "from": "_Production",
                "sharding": [
                    "Scenario"
                ]
            },
            {
                "$": "filter",
                "column": "TECHNOLOGY:Service",
                "in": [
                    "PSG"
                ]
            },
            {
                "$": "aggregate",
                "groupby": [
                    "YEAR",
                    "TECHNOLOGY:TraMode"
                ]
            },
            {
                "$": "save",
                "to": "Transport2A",
                "sharding": [
                    "Scenario"
                ]
            }
        ]
    ]
}
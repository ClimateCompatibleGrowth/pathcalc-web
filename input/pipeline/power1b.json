{
    "sharding": [
        "Scenario"
    ],
    "flows": [
        [
            {
                "$": "load",
                "from": "_TotalCapacity",
                "sharding": [
                    "Scenario"
                ]
            },
            {
                "$": "filter",
                "column": "TECHNOLOGY:Sector",
                "in": [
                    "PWR"
                ]
            },
            {
                "$": "filter",
                "column": "TECHNOLOGY:SubSector",
                "in": [
                    "GEN"
                ]
            },
            {
                "$": "aggregate",
                "groupby": [
                    "YEAR",
                    "TECHNOLOGY:Type"
                ]
            },
            {
                "$": "save",
                "to": "Power1B",
                "sharding": [
                    "Scenario"
                ]
            }
        ]
    ]
}